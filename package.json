    "passport-google-oauth20": "^2.0.0",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.6",
    "sequelize-typescript": "^2.1.6",
    "supertest": "^7.0.0",
